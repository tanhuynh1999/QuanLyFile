@model IEnumerable<Model.EF.FileMain>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.file_circular)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_form)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_startday)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_endday)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_datecreate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_key)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.file_circular)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.file_form)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.file_startday)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.file_endday)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.file_datecreate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.file_status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.file_key)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.file_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.file_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.file_id })
        </td>
    </tr>
}

</table>
