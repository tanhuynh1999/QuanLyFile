                let position = e.target.getAttribute("data-position"); let currentnode = e.target
                let elementwidth = currentnode.clientWidth; let elementheight = currentnode.clientHeight
                let offset

                if (position === "left") {
                    offset = currentnode.getBoundingClientRect().left;
                    if (offset < elementwidth) { console.log("no room! " + offset) }
                } else if (position === "right") {
                    offset = document.documentElement.clientWidth - currentnode.getBoundingClientRect().right
                    if (offset < elementwidth) { console.log("no room! " + offset) }
                } else if (position === "top") {
                    offset = currentnode.getBoundingClientRect().top;
                    if (offset < elementheight) { console.log("no room! " + offset) }
                } else if (position === "bottom") {
                    offset = document.documentElement.clientHeight - currentnode.getBoundingClientRect().bottom;
